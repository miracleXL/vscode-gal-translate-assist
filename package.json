{
	"name": "gal-translate-assist",
	"displayName": "GAL翻译助手",
	"description": "沪江字典查询，划词翻译",
	"version": "0.0.2",
	"publisher": "miracleXL",
	"author": {
		"name": "miracleXL",
		"email": "1250036141@qq.com"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gal-translate-assist.dict",
				"title": "字典查询"
			}
		],
		"keybindings": [
			{
				"command": "gal-translate-assist.dict",
				"key": "ctrl+shift+t",
				"when": "editorHasSelection"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "gal-translate-assist.dict",
					"group": "translate@1"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"build": "webpack",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"clean-webpack-plugin": "^3.0.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^5.8.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"request": "^2.88.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/miracleXL/vscode-gal-translate-assist"
	},
	"homepage": "https://github.com/miracleXL/vscode-gal-translate-assist/blob/main/README.md"
}

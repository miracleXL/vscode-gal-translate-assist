{
	"name": "gal-translate-assist",
	"displayName": "GAL翻译助手",
	"description": "为方便日语galgame汉化，整合百度机翻和沪江字典查询功能",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gal-translate-assist.dict",
				"title": "字典查询"
			}
		],
		"keybindings":[
			{
				"command": "gal-translate-assist.dict",
				"key": "ctrl+shift+t",
				"when": "editorHasSelection"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "gal-translate-assist.dict",
					"group": "translate@1"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"request": "^2.88.2"
	}
}

{
	"name": "yume",
	"displayName": "Yume —— gal翻译助手",
	"description": "使用vscode翻译galgame的辅助插件",
	"version": "0.0.2",
	"publisher": "miracleXL",
	"author": {
		"name": "miracleXL",
		"email": "1250036141@qq.com"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "yume.jpdict",
				"title": "日语字典查询"
			},
			{
				"command": "yume.searchMydict",
				"title": "查询自定义名词表"
			},
			{
				"command": "yume.addToMydict",
				"title": "添加至自定义名词表"
			},
			{
				"command": "yume.translate",
				"title": "百度翻译"
			}
		],
		"keybindings": [
			{
				"command": "yume.jpdict",
				"key": "ctrl+alt+w",
				"when": "editorHasSelection"
			},
			{
				"command": "yume.translate",
				"key": "ctrl+alt+s",
				"when": "editorHasSelection"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "yume.jpdict",
					"group": "gta@1"
				},
				{
					"when": "editorHasSelection",
					"command": "yume.searchMydict",
					"group": "gta@2"
				},
				{
					"when": "editorHasSelection",
					"command": "yume.addToMydict",
					"group": "gta@3"
				},
				{
					"when": "editorHasSelection",
					"command": "yume.translate",
					"group": "gta@4"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"build": "webpack",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"clean-webpack-plugin": "^3.0.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^5.8.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"request": "^2.88.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/miracleXL/vscode-yume"
	},
	"homepage": "https://github.com/miracleXL/vscode-yume/blob/main/README.md"
}
